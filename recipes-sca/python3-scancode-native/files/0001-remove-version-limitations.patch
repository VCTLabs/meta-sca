From b92f89feeaec635470ede588a9257d9b72581b41 Mon Sep 17 00:00:00 2001
From: Konrad Weihmann <kweihmann@outlook.com>
Date: Sun, 29 Nov 2020 21:59:38 +0100
Subject: [PATCH] remove version limitations

on
- pygments
- typing
- ftfy
- bitarray
- fingerprints

Signed-off-by: Konrad Weihmann <kweihmann@outlook.com>
---
 setup.py | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/setup.py b/setup.py
index d7f026417..a3f43c9db 100644
--- a/setup.py
+++ b/setup.py
@@ -140,7 +140,7 @@ setup(
         'py2_ipaddress >= 2.0, <3.5; python_version < "3"',
         'urlpy',
         'publicsuffix2',
-        'fingerprints >= 0.6.0, < 1.0.0',
+        'fingerprints >= 0.6.0',
 
         # commoncode
         'commoncode >= 20.09',
@@ -153,7 +153,7 @@ setup(
         'plugincode',
 
         # licensedcode
-        'bitarray >= 0.8.1, < 1.0.0',
+        'bitarray >= 0.8.1',
         'intbitset >= 2.3.0,  < 3.0',
         'boolean.py >= 3.5,  < 4.0',
         'license_expression >= 0.99',
@@ -185,11 +185,11 @@ setup(
         'gemfileparser >= 0.7.0',
         'pkginfo >= 1.5.0.1',
         'dparse2',
-        'pygments >= 2.4.2, <2.5.1',
+        'pygments >= 2.4.2',
 
         # used to fix mojibake in Windows PE
         # for now we use the evrsion that works on both Python 2 and 3
-        'ftfy <  5.0.0',
+        'ftfy',
 
         # scancode
         # Click 7.0 is broken https://github.com/pallets/click/issues/1125
@@ -197,7 +197,7 @@ setup(
         'colorama >= 0.3.9',
         'pluggy >= 0.4.0, < 1.0',
         'attrs >= 18.1, !=20.1.0',
-        'typing >=3.6, < 3.7',
+        'typing >=3.6',
 
         # scancode outputs
         'jinja2 >= 2.7.0, < 3.0.0',
-- 
2.25.1

